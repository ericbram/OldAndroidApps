<?xml version="1.0" encoding="utf-8"?>
    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" 
    android:layout_height="fill_parent" 
    android:id="@+id/fm_main" android:layout_width="match_parent">
     <TableLayout android:id="@+id/tableLayout1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="bottom">
            <LinearLayout android:id="@+id/linearLayout3" android:orientation="horizontal" android:layout_height="40px" android:layout_width="fill_parent" android:layout_gravity="top">
                <Button android:layout_height="fill_parent" android:id="@+id/btn_1" android:text="1" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating1"></Button>
                <Button android:layout_height="fill_parent" android:id="@+id/btn_2" android:text="2" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating2"></Button>
                <Button android:layout_height="fill_parent" android:id="@+id/btn_3" android:text="3" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating3"></Button>
                <Button android:layout_height="fill_parent" android:id="@+id/btn_4" android:text="4" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating4"></Button>
                <Button android:layout_height="fill_parent" android:id="@+id/btn_5" android:text="5" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating5"></Button>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout1" android:orientation="horizontal" android:layout_height="40px" android:layout_width="fill_parent" android:layout_gravity="bottom">
                <Button android:layout_height="fill_parent" android:id="@+id/btn_6" android:text="6" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating6"></Button>
                <Button android:layout_height="fill_parent" android:id="@+id/btn_7" android:text="7" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating7"></Button>
                <Button android:layout_height="fill_parent" android:id="@+id/btn_8" android:text="8" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating8"></Button>
                <Button android:layout_height="fill_parent" android:id="@+id/btn_9" android:text="9" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating9"></Button>
                <Button android:layout_height="fill_parent" android:id="@+id/btn_10" android:text="10" android:gravity="center_horizontal" android:layout_width="wrap_content" android:onClick="SetRating10"></Button>
            </LinearLayout>
        </TableLayout>
     <TableLayout android:layout_height="wrap_content" android:layout_width="match_parent" android:id="@+id/tableLayout2">
         <TextView android:id="@+id/tv_topLabel" android:gravity="center_horizontal" android:layout_gravity="top" android:textColor="#23cf34" android:textSize="20sp" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="Poop Raps"></TextView>
         <TextView android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/lbl_Author" android:text="@string/author"></TextView>
         <TextView android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/lbl_Date" android:text="@string/date"></TextView>
         <TextView android:id="@+id/lbl_Rap" android:layout_height="wrap_content" android:layout_width="match_parent" android:text="@string/blank"></TextView>
     </TableLayout>
        
    
   </FrameLayout>
